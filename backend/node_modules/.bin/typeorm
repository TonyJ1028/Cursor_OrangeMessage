#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.11.5_ts-node@10.9.2_@types+node@20.17.10_typescript@4.9.5_/node_modules/typeorm/node_modules:/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.11.5_ts-node@10.9.2_@types+node@20.17.10_typescript@4.9.5_/node_modules:/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.11.5_ts-node@10.9.2_@types+node@20.17.10_typescript@4.9.5_/node_modules/typeorm/node_modules:/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.11.5_ts-node@10.9.2_@types+node@20.17.10_typescript@4.9.5_/node_modules:/Users/tonyjia1028/Documents/cursor/chat_app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
